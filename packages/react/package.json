{
  "name": "@tagaddod-design/react",
  "version": "0.1.0",
  "description": "Modern, accessible React component library for building consistent UI at scale.",
  "homepage": "https://tagaddod-design-system.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedamr-r/tagaddod-design-system/tree/main/packages/react"
  },
  "bugs": {
    "url": "https://github.com/ahmedamr-r/tagaddod-design-system/issues"
  },
  "keywords": [
    "react",
    "ui",
    "component-library",
    "design-system",
    "storybook",
    "accessible",
    "LLM",
    "AI",
    "documentation"
  ],
  "docs": "https://tagaddod-design-system.vercel.app/",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "!dist/**/*.map"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./styles": "./dist/styles.css",
    "./AspectRatio": {
      "types": "./dist/types/components/AspectRatio/index.d.ts",
      "import": "./dist/es/components/AspectRatio/index.js",
      "require": "./dist/cjs/components/AspectRatio/index.cjs"
    },
    "./Avatar": {
      "types": "./dist/types/components/Avatar/index.d.ts",
      "import": "./dist/es/components/Avatar/index.js",
      "require": "./dist/cjs/components/Avatar/index.cjs"
    },
    "./Badge": {
      "types": "./dist/types/components/Badge/index.d.ts",
      "import": "./dist/es/components/Badge/index.js",
      "require": "./dist/cjs/components/Badge/index.cjs"
    },
    "./Button": {
      "types": "./dist/types/components/Button/index.d.ts",
      "import": "./dist/es/components/Button/index.js",
      "require": "./dist/cjs/components/Button/index.cjs"
    },
    "./Checkbox": {
      "types": "./dist/types/components/Checkbox/index.d.ts",
      "import": "./dist/es/components/Checkbox/index.js",
      "require": "./dist/cjs/components/Checkbox/index.cjs"
    },
    "./Drawer": {
      "types": "./dist/types/components/Drawer/index.d.ts",
      "import": "./dist/es/components/Drawer/index.js",
      "require": "./dist/cjs/components/Drawer/index.cjs"
    },
    "./Listbox": {
      "types": "./dist/types/components/Listbox/index.d.ts",
      "import": "./dist/es/components/Listbox/index.js",
      "require": "./dist/cjs/components/Listbox/index.cjs"
    },
    "./Modal": {
      "types": "./dist/types/components/Modal/index.d.ts",
      "import": "./dist/es/components/Modal/index.js",
      "require": "./dist/cjs/components/Modal/index.cjs"
    },
    "./Pagination": {
      "types": "./dist/types/components/Pagination/index.d.ts",
      "import": "./dist/es/components/Pagination/index.js",
      "require": "./dist/cjs/components/Pagination/index.cjs"
    },
    "./Popover": {
      "types": "./dist/types/components/Popover/index.d.ts",
      "import": "./dist/es/components/Popover/index.js",
      "require": "./dist/cjs/components/Popover/index.cjs"
    },
    "./RadioButton": {
      "types": "./dist/types/components/RadioButton/index.d.ts",
      "import": "./dist/es/components/RadioButton/index.js",
      "require": "./dist/cjs/components/RadioButton/index.cjs"
    },
    "./Select": {
      "types": "./dist/types/components/Select/index.d.ts",
      "import": "./dist/es/components/Select/index.js",
      "require": "./dist/cjs/components/Select/index.cjs"
    },
    "./Separator": {
      "types": "./dist/types/components/Separator/index.d.ts",
      "import": "./dist/es/components/Separator/index.js",
      "require": "./dist/cjs/components/Separator/index.cjs"
    },
    "./Switch": {
      "types": "./dist/types/components/Switch/index.d.ts",
      "import": "./dist/es/components/Switch/index.js",
      "require": "./dist/cjs/components/Switch/index.cjs"
    },
    "./Table": {
      "types": "./dist/types/components/Table/index.d.ts",
      "import": "./dist/es/components/Table/index.js",
      "require": "./dist/cjs/components/Table/index.cjs"
    },
    "./Tabs": {
      "types": "./dist/types/components/Tabs/index.d.ts",
      "import": "./dist/es/components/Tabs/index.js",
      "require": "./dist/cjs/components/Tabs/index.cjs"
    },
    "./TextInput": {
      "types": "./dist/types/components/TextInput/index.d.ts",
      "import": "./dist/es/components/TextInput/index.js",
      "require": "./dist/cjs/components/TextInput/index.cjs"
    }
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "node scripts/build.cjs",
    "build:css": "node scripts/build-css-standalone.mjs",
    "dev": "vite",
    "watch": "concurrently -n 'vite,css' -c 'yellow,green' 'vite build --watch' 'node scripts/watch-css.js'",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "diagnose": "node scripts/diagnose-build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:index-coverage": "jest --coverage --collectCoverageFrom='**/index.ts'",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-form": "^0.1.6",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@tabler/icons-react": "^3.33.0",
    "@tanstack/react-table": "8",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "glob": "^11.0.2",
    "i18next": "^25.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.1",
    "postcss-preset-env": "^10.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "rollup-plugin-dts": "^6.2.1",
    "terser": "^5.30.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module"
}