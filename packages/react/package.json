{
  "name": "@tagaddod-design/react",
  "version": "0.1.15",
  "description": "Modern, accessible React component library for building consistent UI at scale.",
  "homepage": "https://tagaddod-design-system.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedamr-r/tagaddod-design-system/tree/main/packages/react"
  },
  "bugs": {
    "url": "https://github.com/ahmedamr-r/tagaddod-design-system/issues"
  },
  "keywords": [
    "react",
    "ui",
    "component-library",
    "design-system",
    "storybook",
    "accessible",
    "LLM",
    "AI",
    "documentation"
  ],
  "docs": "https://tagaddod-design-system.vercel.app/",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/styles/styles.css",
  "files": [
    "dist",
    "!dist/**/*.map",
    "!dist/**/*.tsbuildinfo",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./styles": {
      "default": "./dist/styles/styles.css"
    },
    "./styles.css": {
      "default": "./dist/styles/styles.css"
    },
    "./styles/*": {
      "default": "./dist/styles/*"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "node scripts/build.cjs",
    "build:css": "node scripts/build-css.js",
    "test:css-injection": "node scripts/test-css-injection.cjs",
    "dev": "vite",
    "watch": "vite build --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "yarn build",
    "generate-llm-txt": "node scripts/generate-llm-txt.cjs",
    "generate-llm-txt:simple": "node scripts/generate-llm-txt-simple.cjs",
    "prebuild:docs": "yarn generate-llm-txt:simple"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-form": "^0.1.6",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@tabler/icons-react": "^3.33.0",
    "@tanstack/react-table": "^8.0.0",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-jest": "^29.7.0",
    "glob": "^11.0.2",
    "gray-matter": "^4.0.3",
    "i18next": "^25.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.1",
    "postcss-preset-env": "^10.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "remark-mdx": "^3.1.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "terser": "^5.30.0",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module"
}