{
  "name": "@tagaddod/react",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/styles/index.css",
  "files": [
    "dist",
    "!dist/**/*.map"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles/index.css": "./dist/styles/index.css",
    "./styles.css": "./dist/styles/index.css"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "vite build && tsc --project tsconfig.build.json && yarn build:css",
    "postbuild": "echo 'Build complete with integrated tokens'",
    "dev": "vite",
    "watch": "concurrently -n 'vite,css' -c 'yellow,green' 'vite build --watch' 'yarn watch:css'",
    "watch:css": "node scripts/watch-css.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "build:css": "node scripts/build-css.js"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-form": "^0.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@tabler/icons-react": "^3.31.0",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "i18next": "^25.1.2",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.1",
    "postcss-preset-env": "^10.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  }
}
