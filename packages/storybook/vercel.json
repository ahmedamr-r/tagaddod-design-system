{
  "buildCommand": "cd ../.. && yarn build:tokens && yarn build:react && cd packages/storybook && yarn vercel-build",
  "outputDirectory": "storybook-static",
  "installCommand": "cd ../.. && yarn install",
  "framework": null,
  "rewrites": [
    {
      "source": "/llms.txt",
      "destination": "/llms.txt"
    }
  ],
  "headers": [
    {
      "source": "/llms.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
