{
  "duration": {
    "fast": {
      "$value": "150ms",
      "$type": "duration"
    },
    "base": {
      "$value": "300ms",
      "$type": "duration"
    }
  },
  "easing": {
    "inOut": {
      "$value": [
        0.4,
        0,
        0.2,
        1
      ],
      "$type": "cubicBezier"
    }
  },
  "transition": {
    "soft": {
      "$value": {
        "duration": "{duration.base}",
        "timingFunction": "{easing.inOut}"
      },
      "$type": "transition"
    }
  }
}