{
  "name": "tagaddod-design-system",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "ahmedamr-r <ahmedamr6416@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run watch --parallel",
    "build": "turbo run build",
    "storybook": "turbo run storybook --filter=@tagaddod/storybook",
    "build-storybook": "turbo run build-storybook --filter=@tagaddod/storybook",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules/.cache",
    "release": "changeset version && turbo run build && changeset publish",
    "changeset": "changeset"
  },
  "devDependencies": {
    "turbo": "^2.5.3",
    "@changesets/cli": "^2.27.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "@storybook/core": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/preview-api": "^8.0.0"
  }
}
